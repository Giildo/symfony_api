{
  "name": "jojotique/api",
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.2.2",
  "description": "Build API with Symfony 4",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "nelmio/api-doc-bundle": "^3.5",
    "symfony/twig-pack": "^1.0",
    "symfony/orm-pack": "^1.0",
    "symfony/serializer": "4.4.*",
    "symfony/validator": "4.4.*"
  },
  "authors": [
    {
      "name": "Giildo",
      "email": "giildo.jm@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jojotique\\Api\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
